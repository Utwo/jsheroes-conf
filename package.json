    "@angular/core": "^6.0.0-rc.6",
    "@angular/forms": "^6.0.0-rc.6",
    "@angular/http": "^6.0.0-rc.6",
    "@angular/material": "^6.0.0-rc.13",
    "@angular/platform-browser": "^6.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.6",
